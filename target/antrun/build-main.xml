<?xml version="1.0" encoding="UTF-8" ?>
<project name="maven-antrun-" default="main"  >
<target name="main">
  <echo></echo>
  <echo>*******************************************</echo>
  <echo>Inicia la tarea ANT.</echo>
  <tstamp>
    <format pattern="yyyyMMddHHmm" property="build.time"/>
  </tstamp>
  <tstamp>
    <format pattern="yyyyMMdd.HHmm" property="build.timeNew"/>
  </tstamp>
  <tstamp>
    <format pattern="HHmm" property="build.time.hora"/>
  </tstamp>
  <tstamp>
    <format pattern="yyyyMMdd" property="build.time.dia"/>
  </tstamp>
  <property name="pathProyectoLocal" value="c://_proyectos//PES//deploy"/>
  <property name="pathServidorRemotoProduccion" value="p://"/>
  <property name="pathServidorRemotoTesting" value="t://"/>
  <echo>Finaliza la tarea ANT.</echo>
  <echo>*******************************************</echo>
  <echo></echo>
</target>
</project>