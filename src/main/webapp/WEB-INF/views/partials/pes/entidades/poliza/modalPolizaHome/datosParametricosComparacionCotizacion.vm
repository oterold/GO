<div class="panel panel-default" style="display:none;max-width:800px;height:500px;overflow-y:hidden;overflow-x:hidden;" id="datosParametricosCMP" >
    <div style="width:100%;">
        <a class="btn pull-right cerrarModalBotonHoover"  data-placement="bottom"  style="font-size:20px;margin-bottom:5px;height:30px;padding-bottom:5px;padding-top:1px;color:#989898;background-color:#ffffff;box-shadow:none;" rel="modal:close">
            <i style="font-size:17px;" class="fas fa-times"></i>
        </a>
    </div>
    <div class="panel-heading" style="max-width:790px;background-color:#ffffff;border-color:#ffffff;">
        <div class="row">
            <div class="col-md-4">
                <h4 class="modal-title" id="" style="border:none;">Datos Parametricos</h4>
            </div>
            <div class="col-md-8">
				<div class="form-group">
                    <select class="form-control" id="selectorDatosParametricosCMPA" onchange="mostrarParametricosCMP()";>
                      <option></option>
                    </select>
                </div>
            </div>
            <div class="col-md-3 ml-1">
                <h4 id="cantIgualParamLabel" onclick="mostrarDeferenciasCabecera('iconoDiferenciasDatosParam','cantIgualParamLabel');" class="bordeFiltro" style="cursor:pointer;border-bottom-width:0px;font-size: 12px;font-weight: 400;"></h4>
            </div>
            <div class="col-md-3">
                <h4 id="cantDifParamLabel" onclick="mostrarDeferenciasCabecera('iconoIgualDatosParam','cantDifParamLabel');" class="bordeFiltro" style="cursor:pointer;border-bottom-width:0px;font-size: 12px;font-weight: 400;"></h4>
            </div>
        </div>
    </div>
    <div>
        <div class="row" id="mensajeDatosParametricos">
            <div class="col-md-12" style="text-align:center;height:400px;">
                <h5 style="border:none;"> Seleccione un bien..</h5>
            </div>
        </div>
        <div class="row" id="mostrarTablaParametricos">
            <div class="col-md-12" style="text-align: left; padding-left:1px;padding-right:1px">
                <div style="overflow-y:auto;height:400px;">
                    <table  class="table table-hover" style="width:100%;">
                        <thead class="thead-grilla" role="rowgroup">
                            <tr class="tr-grilla" role="row">
                                <th  scope="col" role="columnheader" style="width:30%;text-align: right;">
                                    <h5 style="border:none;">Dato</h5>
                                </th>
                                <th  scope="col" style="text-align: center;width: 27%;" role="columnheader">
                                    <h5 style="border:none;">Cotizaci&oacute;n A (<a style="color:#bfbac2;" class="cotizacionA">Sin dato</a>)</h5>
                                </th>
                                <th  scope="col" style="text-align: center;width: 15%;" role="columnheader"></th>
                                <th  scope="col" style="text-align: center;width: 27%;" role="columnheader">
                                    <h5 style="border:none;">Cotizaci&oacute;n B (<a style="color:#bfbac2;" class="cotizacionB">Sin dato</a>
                                        )</h5>
                                </th>
                            </tr>
                        </thead>
                        <tbody class="tbody-grilla" id="tablaDatosParametricos">
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
